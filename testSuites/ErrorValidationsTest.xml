<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	
	<groups>
		
		<run>
			
			<include name="ErrorHanding"></include>
			
		</run>
		
	</groups>
	
	
	
  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="supratimdas.SeleniumFrameworkDesign.SubmitOrderTest"/>
    </classes>
    </test>
    
    
    <test thread-count="5" name="Error Validation Test">
    <classes>
      <class name="supratimdas.SeleniumFrameworkDesign.ErrorValidationsTest"/>
    </classes>
    </test>
    
   <test thread-count="5" name="Base Validation Test">
    <classes>
      <class name="supratimdas.TestComponents.BaseTest"/>
    </classes>
    </test> 
    
    
    
    
</suite> <!-- Suite -->

